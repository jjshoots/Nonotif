{
    "name": "Nonotif",
    "description" : "Hides Facebook Notifications",
    "version": "1.0",
    "manifest_version": 2,
    "icons": {
        "16": "assets/nyan16.png",
        "48": "assets/nyan48.png",
        "128": "assets/nyan128.png"},
    "content_scripts": [
        {
          "matches": [
            "https://www.facebook.com/*"
          ],
          "js": ["scripts/functions.js", "scripts/main.js"]
        }
      ]
  }